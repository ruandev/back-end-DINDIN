  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/moment": "^2.13.0",
    "@types/node": "^18.11.15",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@types/yargs": "^17.0.17",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.7",
    "typeorm-extension": "^2.1.5"
  },
  "scripts": {
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/data-source.ts",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run",
    "build": "tsc",
    "start": "node --loader ts-node/esm --experimental-modules --es-module-specifier-resolution=node --require dotenv/config ./dist/index.js"
  }
}